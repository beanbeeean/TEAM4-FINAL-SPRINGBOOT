<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.office.libooksserver.reservation.readroom.ReadRoomMapper">

    <select id="checkSeat" resultType="com.office.libooksserver.reservation.readroom.ReadRoomDto">

        SELECT * FROM tbl_seat

    </select>

    <insert id="reservationSeat" >

        INSERT INTO tbl_seat (re_room_no, s_name, s_reservation)
        VALUES (#{userEmail}, #{refreshToken},now())

    </insert>


</mapper>